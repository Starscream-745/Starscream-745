#include<stdio.h>
void main(){
int temp,j,i,n,array[20];
printf("enter the number of elements in an array\n");
scanf("%d",&n);
printf("enter the array\n");
for(i=0;i<n;i++)
{
scanf("%d",&array[i]);
}
for (i = 0; i< n - 1; ++i) 
{
for ( j= 0; j < n - i - 1; ++j)
{
      if (array[j] > array[j + 1]) 
      {
     temp = array[j];
        array[j] = array[j + 1];
        array[j + 1] = temp;
      }
    }
  }
  for (int i = 0; i < n; ++i) 
  {
    printf("%d  ", array[i]);
  }
  }
